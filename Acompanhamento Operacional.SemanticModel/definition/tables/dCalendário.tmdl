	column 'É Mês Vigente' =
			
			IF('dCalendário'[ANO-MÊS] = FORMAT(TODAY(), "YYYY-MM"), "Sim", "Não")
		lineageTag: ee138731-94b1-4efb-94d5-688d54059834
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	partition dCalendário = calculated
		mode: import
		source =
				
				ADDCOLUMNS(
				    CALENDAR(
				        MIN('fColetas'[DATA_INCLUSAO]),
				        EOMONTH(MAX('fColetas'[DATA_INCLUSAO]), 0)),
				        "ANO",YEAR([Date]),
				        "MÊS", MONTH([Date]),
				        "DIA", DAY([Date]),
				        "ANO-MÊS", FORMAT([Date],"YYYY-MM"),
				        "ANO-NOME MÊS", UPPER(FORMAT([Date],"MMM-YY")),
				        "NOME DIA DA SEMANA", UPPER(FORMAT([Date],"DDDD")),
				        "DIA DA SEMANA", WEEKDAY([Date]),
				        "NOME DO MÊS", FORMAT([Date], "MMMM"),
				        "TRIMESTRE", FORMAT([Date],"Q") & " TRIMESTRE",
				        "CICLO", SWITCH(TRUE(),
				            DAY([Date]) <= 7, "1 SEMANA",
				            DAY([Date]) <= 14, "2 SEMANA",
				            DAY([Date]) <= 21, "3 SEMANA",
				            DAY([Date]) >= 22, "4 SEMANA")
				)

	annotation PBI_Id = c0b5e556211c4c90aaa17b703e893846

